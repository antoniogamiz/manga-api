{
  "compilerOptions": {
    "module": "amd",
    "target": "esnext",
    "baseUrl": ".",
    "importsNotUsedAsValues": "preserve",
    "isolatedModules": false,
    "paths": {
      "http://*": ["../../../.deno/deps/http/*"],
      "https://*": ["../../../.deno/deps/https/*"],
      "*.ts": ["*"]
    },
    "plugins": [
      {
        "name": "typescript-deno-plugin"
      }
    ],
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true
  }
}
